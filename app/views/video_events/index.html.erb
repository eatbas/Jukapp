<% content_for :title, "Home" %>

<paper-tabs selected="0" class="paper-tabs">
  <%= render "shared/common_tabs" %>
</paper-tabs>

<%= render "shared/common_tab_contents" %>

<script>
  $(function() {
    var eventStream = new EventStream(<%= event_stream_params %>);
    eventStream.forQueue();

    VideoOperations.fetchRows("<%= recents_path %>", "#recents-tab-content");
    $("#recents-tab-content").show();
  });
</script>
